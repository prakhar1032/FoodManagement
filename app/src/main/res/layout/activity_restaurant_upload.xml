<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".restaurant.RestaurantUpload">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbarrestaurantback">
    </include>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="307dp"
        android:layout_height="49dp"
        android:text="Enter Item Details"
        android:textColor="#FF5722"
        android:textSize="16pt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.132" />

    <EditText
        android:id="@+id/etitemname"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:ems="10"
        android:hint=" Enter item name"
        android:inputType="textPersonName"
        android:singleLine="true"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.065" />

    <EditText
        android:id="@+id/datep"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:ems="10"
        android:focusable="false"
        android:hint=" Date (when prepared)"
        android:inputType="date"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etquantity"
        app:layout_constraintVertical_bias="0.029" />

    <EditText
        android:id="@+id/timep"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:focusable="false"
        android:hint=" Time  (when prepared)"
        android:inputType="time"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/datep"
        app:layout_constraintVertical_bias="0.045" />

    <EditText
        android:id="@+id/etquantity"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:ems="10"
        android:hint=" Quantity"
        android:inputType="textShortMessage|textPersonName"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etitemname"
        app:layout_constraintVertical_bias="0.021" />

    <EditText
        android:id="@+id/datef"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:ems="10"
        android:focusable="false"
        android:hint=" Date(Till it is fresh)"
        android:inputType="date"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timep"
        app:layout_constraintVertical_bias="0.049" />

    <EditText
        android:id="@+id/timef"
        android:layout_width="329dp"
        android:layout_height="56dp"
        android:background="#FABEAB"
        android:ems="10"
        android:focusable="false"
        android:hint=" Time(Till it is fresh)"
        android:inputType="time"
        android:textColor="@color/black"
        android:textColorHint="#686767"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/datef"
        app:layout_constraintVertical_bias="0.065" />

    <Button
        android:id="@+id/btrupload"
        android:layout_width="197dp"
        android:layout_height="76dp"
        android:text="Upload"
        android:textColor="#FFFFFF"
        app:backgroundTint="#9EFF5722"
        app:iconTint="#1C1B1B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timef"
        app:layout_constraintVertical_bias="0.492" />

</androidx.constraintlayout.widget.ConstraintLayout>